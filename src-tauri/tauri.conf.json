{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "sptraderb",
  "version": "0.1.0",
  "identifier": "com.sptraderb.app",
  "build": {
    "beforeDevCommand": "cd ../demo && npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "cd ../demo && npm run build",
    "frontendDist": "../demo/dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "SPTrader",
        "width": 1200,
        "height": 800,
        "visible": true,
        "fullscreen": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}